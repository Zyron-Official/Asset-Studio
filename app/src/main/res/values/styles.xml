<resources xmlns:tools="http://schemas.android.com/tools">
    
    <!-- Style - PopupMenu - Asset Studio -->
   
    <!-- Style - MaterialAlertDialog - Asset Studio -->
    <style name="MaterialAlertDialog.App" parent="MaterialAlertDialog.Material3">
        <item name="backgroundTint">?attr/colorAlertDialog</item>
        <item name="shapeAppearance">@style/ShapeAppearance.App.MediumComponent</item>
        <item name="shapeAppearanceOverlay">@null</item>
    </style>
    <!-- Style - TitleText- MaterialAlertDialog -->
    <style name="MaterialAlertDialog.App.Title.Text" parent="MaterialAlertDialog.Material3.Title.Text">
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:textAppearance">?attr/textAppearanceTitleLarge</item>
    </style>
    <!-- Style - BodyText - MaterialAlertDialog -->    
    <style name="MaterialAlertDialog.App.Body.Text" parent="MaterialAlertDialog.Material3.Body.Text">
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:textAppearance">?attr/textAppearanceBodyLarge</item>
    </style>
    <!-- Style - Button - MaterialAlertDialog -->
    <style name="Widget.App.Button" parent="Widget.Material3.Button.TextButton.Dialog">
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>
    <!-- Style - ShapeAppearance - MaterialAlertDialog -->
    <style name="ShapeAppearance.App.MediumComponent" parent="ShapeAppearance.Material3.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">4dp</item>
    </style>
    
    <!-- Style - BottomSheetDialog - Asset Studio-->
    <style name="BottomSheetDialog.App" parent="Widget.Material3.BottomSheet">
        <item name="backgroundTint">?attr/colorBottomSheet</item>
        <item name="android:navigationBarColor">?attr/colorBottomSheet</item>       
        <item name="shapeAppearance">@style/ShapeAppearance.App.LargeComponent</item>
    </style>
    <!-- Style - ShapeAppearance - BottomSheetDialog -->
    <style name="ShapeAppearance.App.LargeComponent" parent="ShapeAppearance.Material3.LargeComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">24dp</item>
    </style>    

    <!-- Style - NavigationDrawer - Asset Studio -->    
    <style name="ThemeOverlay.App.NavigationView" parent="Widget.Material3.NavigationView">
        <item name="textAppearanceTitleSmall">@style/TextAppearance.Material3.TitleSmall</item>
        <item name="textAppearanceLabelLarge">@style/TextAppearance.Material3.LabelLarge</item>
    </style>
    
    <!-- Style - Typography - Aaset Studio -->
